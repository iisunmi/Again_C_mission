#include <stdio.h>

#define _CRT_SECURE_NO_WARNINGS
#pragma warning(disable: 4996)


void main11(){
//1. 10개 정수 입력 후 최대 최소 총합 평균을 구하세요.
	
	double num=0;
	double min=1;
	double max=0;
	double sum=0;
	double avg;
	

	
	for (int i = 1; i <= 10; ++i) {
		
		printf("숫자를 입력하세요 >> ");
		scanf("%lf", &num);
		if (min > num){
			min = num;
		}

		if (max < num){
			max = num;
		}
		
			sum += num;
			avg = sum / 10;
	}
	printf("총합 : %lf\n", sum);
	printf("평균 : %.2lf\n", avg);
	printf("최대 : %lf\n", max);
	printf("최소 : %lf\n", min);

	
}


